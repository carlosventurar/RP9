    "@fastify/rate-limit": "^8.0.3",
    "@fastify/auth": "^4.4.0",
    "dockerode": "^4.0.0",
    "pino": "^8.16.2",
    "pino-pretty": "^10.2.3",
    "zod": "^3.22.4",
    "jsonwebtoken": "^9.0.2",
    "crypto": "^1.0.1",
    "node-cron": "^3.0.3",
    "@supabase/supabase-js": "^2.38.4",
    "stripe": "^14.7.0",
